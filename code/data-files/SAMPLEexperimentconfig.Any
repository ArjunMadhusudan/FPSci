// copy this file to experimentconfig.Any before first run
{
  // Information related to this file
  "settingsVersion": 1, // Used for parsing this file

  // General settings for the experiment
  "playMode": true, // Debugging/development control flag
  "appendingDescription": "default", // Description of this file
  "sceneName": "eSports Simple Hallway", // Maybe donï¿½t need this? Or maybe this ends up belonging to individual trials
  "taskType": "target", // Task type, can be "target" or "reaction"
  "feedbackDuration": 1.0, // Time allocated for providing user feedback
  "readyDuration": 0.5, // Time allocated for preparing for trial
  "taskDuration": 100000.0, // Maximum duration allowed for completion of the task
  "maxClicks": 100,
  "sessionOrder": "random", // Session ordering, can be "Random", "Serial"

  // Create a sessions table containing information related to sessions
  // These sessions attempt to have fixed latency with variable frame rate
  // all run the same set of trial conditions
  "sessions": [
    {
      "id": "s1", // Session ID
      "frameDelay": 1, // Session frame delay (in frames)
      "frameRate": 240, // Session frame rate (in frames per second)
      "selectionOrder": "random", // Selection order for trials within this session
      "expMode": "training", // Experiment mode ("real" or "training")
      "trials": [
        {"id": "c1","count": 1},
        {"id": "c2","count": 1},
        {"id": "c3","count": 1}]
    }
    {
      "id": "s2",
      "frameDelay": 2,
      "frameRate": 240,
      "selectionOrder": "random",
      "expMode": "training",
      "trials": [
        {"id": "c1","count": 2},
        {"id": "c2","count": 2},
        {"id": "c3","count": 2}]
    },
    {
      "id": "s3",
      "frameDelay": 3,
      "frameRate": 240,
      "selectionOrder": "random",
      "expMode": "training",
      "trials": [
        {"id": "c1","count": 10},
        {"id": "c2","count": 5},
        {"id": "c3","count": 5}]
    }],

  // c1 = stationary
  // c2 = fixed direction
  // c3 = change direction every 1/2 second
  // c4 = constant speed, changes direction, c5 and c6 similar
  // Trial table (contains detailed target info for each trial)
  "targets": [
    {
      "id": "c1",
      "distance": 30,
      "elevationLocked": false,
      "speed": [ 0, 0 ],
      "visualSize": [ 0.02, 0.02 ],
      "eccH": [ 5.0, 15.0 ],
      "eccV": [ 0.0, 1.0 ],
      "motionChangePeriod": [ 1000000, 1000000 ],
      "jumpEnabled": false
    },
    {
      "id": "c2",
      "distance": 30,
      "elevationLocked": false,
      "speed": [ 8, 15 ],
      "visualSize": [ 0.02, 0.02 ],
      "eccH": [ 5.0, 15.0 ],
      "eccV": [ 0.0, 1.0 ],
      "motionChangePeriod": [ 1000000, 1000000 ],
      "jumpEnabled": false
    },
    {
      "id": "c3",
      "distance": 30,
      "elevationLocked": false,
      "speed": [ 8, 15 ],
      "visualSize": [ 0.02, 0.02 ],
      "eccH": [ 5.0, 15.0 ],
      "eccV": [ 0.0, 1.0 ],
      "motionChangePeriod": [ 0.5, 0.5 ],
      "jumpEnabled": false
    },
    {
      "id": "c4",
      "distance": 30,
      "elevationLocked": true,
      "speed": [ 8, 15 ],
      "visualSize": [ 0.02, 0.02 ],
      "eccH": [ 5.0, 15.0 ],
      "eccV": [ 0.0, 1.0 ],
      "motionChangePeriod": [ 0.5, 0.5 ],
      "jumpEnabled": true,
      "jumpPeriod": [ 1.0, 1.0 ],
      "accelGravity": [ 9.8, 9.8 ]
    }
  ],

  // A set of controls for reaction time experiments
  "reactions": [
    {
      "id": "r1",
      "minimumForeperiod": 1.5,
      "intensities": [ 0.4, 1.0 ],
      "intensityCounts": [ 3, 10 ]
    }
  ]
}

    
